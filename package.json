{
	"name": "capacitor-example-app",
	"version": "1.0.0",
	"description": "An opinionated project template for building mobile apps using CapacitorJS.",
	"author": "Objekt Labs",
	"license": "MIT",
	"homepage": "https://github.com/objektlabs/capacitor-app-updater",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/objektlabs/capacitor-app-updater.git"
	},
	"bugs": {
		"url": "https://github.com/objektlabs/capacitor-app-updater/issues"
	},
	"type": "module",
	"main": "index.js",
	"scripts": {
		"build:dev": "npx webpack build --config ./src/web/config/webpack.dev.js",
		"build:release": "npx webpack build --config ./src/web/config/webpack.release.js",
		"run:web": "npx webpack serve --config ./src/web/config/webpack.dev.js",
		"run:android": "npm run build:dev && npx cap sync android && npx cap run android",
		"run:ios": "npm run build:dev && npx cap sync ios && npx cap run ios",
		"test": "echo \"Error: no test specified\" && exit 0",
		"lint": "npx eslint -c .eslintrc.json ./src"
	},
	"dependencies": {
		"@capacitor/android": "^3.7.0",
		"@capacitor/core": "^3.7.0",
		"@capacitor/device": "^1.1.2",
		"@capacitor/ios": "^3.7.0",
		"@capacitor/splash-screen": "^1.2.2",
		"@objekt/capacitor-app-updater": "^1.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/eslint-parser": "^7.18.9",
		"@babel/eslint-plugin": "^7.18.10",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.18.10",
		"@babel/plugin-proposal-private-methods": "^7.18.6",
		"@babel/plugin-transform-runtime": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@capacitor/cli": "^3.7.0",
		"babel-loader": "^8.2.5",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^4.0.0",
		"eslint": "^8.21.0",
		"eslint-plugin-jsdoc": "^39.3.4",
		"html-webpack-plugin": "^5.5.0",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.3.1",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.3",
		"webpack-merge": "^5.8.0",
		"workbox-webpack-plugin": "^6.5.4"
	}
}
