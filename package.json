		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^4.0.0",
		"eslint": "^8.18.0",
		"eslint-plugin-jsdoc": "^39.3.3",
		"html-webpack-plugin": "^5.5.0",
		"http-server": "^14.1.0",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.3.1",
		"webpack-cli": "^4.9.2",
		"webpack-merge": "^5.8.0",
		"workbox-webpack-plugin": "^6.5.3"
